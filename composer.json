{
  "name": "namshi/cuzzle",
  "description": "Get the cURL shell command from a Guzzle request",
  "require": {
    "php": ">=8.3",
    "guzzlehttp/guzzle": "^7.0",
    "psr/log": "^3.0",
    "guzzlehttp/psr7": "^2.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "monolog/monolog": "^3.8",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^11.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Namshi\\Cuzzle\\": "src/",
      "Namshi\\Cuzzle\\Test\\" : "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "cirpo",
      "email": "alessandro.cinelli@gmail.com"
    },
    {
      "name": "Nikita Nefedov",
      "email": "inefedor@gmail.com"
    },
    {
      "name": "Franck Magnan",
      "email": "fmagnan@vivaldi.net"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
